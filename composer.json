{
    "name": "linuxstreet/laravel-registry",
    "description": ":description",
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Jovanovic",
            "email": "linuxstreet@gmail.com",
            "homepage": "http://www.linuxstreet.net",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/linuxstreet/laravel-registry",
    "keywords": ["Laravel", "Registry"],
    "require": {
        "illuminate/support": "~5",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Linuxstreet\\Registry\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Linuxstreet\\Registry\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Linuxstreet\\Registry\\RegistryServiceProvider"
            ],
            "aliases": {
                "Registry": "Linuxstreet\\Registry\\Facades\\Registry"
            }
        }
    }
}
